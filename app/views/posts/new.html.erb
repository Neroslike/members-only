<%= form_with model: @post do |f| %>
  <ul>
    <% @post.errors.each do |error| %>
      <li><%= error.full_message %></li>
    <% end %>
  </ul>

  <%= f.label :title, "Title" %>
  <%= f.text_field :title %></br></br>
  <%= f.label :body, "Body" %>
  <%= f.text_area :body %></br></br>
  <%= f.submit %>
<% end %>
